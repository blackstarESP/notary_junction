<header>
  <nav class="navbar fixed-top navbar-expand-lg navbar-dark unique-color-dark">
    <a class="navbar-brand" href="#"><strong>Notary Junction</strong></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ml-auto">
        <li class="nav-item active">
          <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">About</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Resources</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Agency Reviews</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Support</a>
        </li>
      </ul>
      <% if user_signed_in? %>
        <%= link_to logout_path, class: "btn btn-primary waves-effect btn-md font-weight-bold", method: :delete do %>
          <i class="fa fa-sign-out left" aria-hidden="true"></i> Log Out
        <% end %>
        <%= link_to "", class: "btn btn-warning waves-effect btn-md font-weight-bold" do %>
          <i class="fa fa-list-ul left" aria-hidden="true"></i> Edit Your Profile
        <% end %>
      <% else %>
        <%= link_to login_path, class: "btn btn-primary waves-effect btn-md font-weight-bold" do %>
          <i class="fa fa-sign-in left" aria-hidden="true"></i> Log In
        <% end %>
        <%= link_to register_path, class: "btn btn-warning waves-effect btn-md font-weight-bold" do %>
          <i class="fa fa-list-ul left" aria-hidden="true"></i> Get Listed for Free
        <% end %>
      <% end %>
    </div>
  </nav>
</header>
